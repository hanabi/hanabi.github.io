{
  "name": "hanabi.github.io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsx --tsconfig ./scripts/tsconfig.json ./scripts/build.mts",
    "check-valid": "tsx --tsconfig ./scripts/tsconfig.json ./scripts/checkValidYAMLFiles.mts",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "format": "prettier --write .",
    "lint": "tsx --tsconfig ./scripts/tsconfig.json ./scripts/lint.mts",
    "nuke": "isaacscript nuke",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "test": "tsx --tsconfig ./scripts/tsconfig.json ./scripts/test.mts",
    "update": "npm-check-updates --upgrade --filter !typescript"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "@mdx-js/react": "3.0.1",
    "prism-react-renderer": "2.4.0",
    "raw-loader": "4.0.2",
    "react": "18.3.1",
    "react-circular-progressbar": "2.1.0",
    "react-dom": "18.3.1",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@docusaurus/tsconfig": "3.5.2",
    "complete-lint": "1.9.2",
    "complete-node": "1.5.1",
    "glob": "^11.0.0",
    "isaacscript": "5.0.6",
    "typescript": "5.5.4",
    "webpack": "5.94.0",
    "xml-formatter": "^3.6.3"
  }
}
